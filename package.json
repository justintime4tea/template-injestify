{
  "name": "template-injestify",
  "version": "0.0.1",
  "description": "A Typescript project template which includes jest and inversify.",
  "main": "dist/src/bin.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "injestify": "./dist/src/bin.js"
  },
  "repository": "https://github.com/JustinTime4Tea/template-injestify",
  "author": "Justin Gross <justin@jgross.tech>",
  "license": "GPL3",
  "private": false,
  "scripts": {
    "start": "node ./dist/src/bin.js",
    "debug": "cross-env LOG_LEVEL=6 node ./dist/src/bin.js",
    "test": "cross-env NODE_ENV=test jest",
    "lint": "eslint src/ --ext .js,.ts",
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist build",
    "ts-node": "ts-node"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "inversify": "^6.0.1",
    "jest": "^27.4.7",
    "module-alias": "^2.2.2",
    "prettier": "^2.5.1",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "tsconfig-paths": "^3.12.0"
  }
}